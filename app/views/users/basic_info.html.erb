<% provide(:title, 'Basic Info') %>
<h1>Add Musician Information</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages' %>

      <%= f.label :MusicianName %>
      <%= f.text_field :name %>

      <%= f.submit "Create Account", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>